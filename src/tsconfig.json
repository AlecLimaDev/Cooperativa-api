    "paths": {
      "@prisma/client": ["node_modules/@prisma/client"]
    },
    "target": "es2020",
    "lib": [],
    "module": "commonjs",
    "rootDir": "./",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  }
}
