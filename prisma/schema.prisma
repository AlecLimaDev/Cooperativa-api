generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Banco {
   id         String    @id @default(cuid())
   cnpj       String 
   nome_instituicao String
   segmento   String
   endereco   String
   complemento String
   bairro     String
   cep        String
   municipio  String
   uf         String
   ddd        String
   fone       String
   cart_comercial   String
   email      String
   municipio_ibge   String
  site_na_internet  String
}



model Cooperativa { 
  id String @id @default(cuid())
  cnpj                String
  nome_da_instituicao String
  endereco            String
  complemento         String  
  bairro              String
  cep                 String    
  municipio           String
  uf                  String
  ddd                 String
  telefone            String
  classe              String
  criterio_de_associacao String
  categ_coop_sing     String
  filiacao            String
  email               String
  site_da_cooperativa String
}

model Sociedade {
  id        String @id @default(cuid())
  cnpj     String
  segmento String
  endereco String
  complemento String
  bairro   String
  cep      String
  cidade   String
  uf       String
  ddd      String
  fone     String
  email    String
  municipio_ibge String
}

model Consorcio {
  id        String @id @default(cuid())
  cnpj                String
  nome_da_instituicao String
  endereco            String
  complemento         String
  bairro              String
  cep                 String
  municipio           String
  uf                  String
  email               String
  municipio_ibge      String
}


model Conglomerado {
  id        String @id @default(cuid())
  nome_do_conglomerado    String
  classe_do_conglomerado  String
  cnpj_participante       String
  nome_do_participante    String
  tipo_participacao       String
  data_inicio            DateTime
  uf                      String
}

model CompeIspb {
  id    String @id @default(cuid())
  compe String
  ispb  String
  documento String
  nome_longo String
  nome_curto String
  network    String
  tipo       String
  tipo_pix   String
  charge     String
  documento_credito String
  cheque_legal      String
  detecta_flow      String
  portabilidade_salario String
  produtos              String
  url                   String
  operacao_iniciada     String
  data_pix_iniciada     String
  data_registrada       String
  data_atualizada       String  
}
